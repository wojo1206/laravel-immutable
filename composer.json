{
  "name": "zablockibros/laravel-immutable",
  "description": "Make model attributes immutable",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Justin Zablocki",
      "email": "zablockijj@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3|^8.0",
    "laravel/framework": "~8.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "ZablockiBros\\Immutable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZablockiBros\\Immutable\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "ZablockiBros\\Immutable\\ComposerScripts::postAutoloadDump"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZablockiBros\\Immutable\\ImmutableServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
